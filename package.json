{
  "name": "elite-panel",
  "version": "0.1.0",
  "description": "A side panel for Elite Dangerous",
  "main": "main.js",
  "build": {
    "appId": "elite-panel",
    "category": "gaming",
    "iconUrl": "https://electron.atom.io/images/favicon.ico",
    "extraFiles": [
      {
        "from": "app/plugins/",
        "to": "plugins/",
        "filter": ["**/*"]
      }
    ]
  },
  "scripts": {
    "start": "cd app/; electron ./"
  },
  "author": "Rahix",
  "license": "MIT",
  "devDependencies": {
    "electron": "^1.4.2",
    "electron-builder": "^7.10.2"
  }
}
